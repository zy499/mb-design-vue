{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    // "lib": ["es2020", "dom", "es2020.promise"],
    "allowJs": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@web-vue/*": [
        "../web-vue/*"
      ]
    },
  }
}
